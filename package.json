    "express": "^4.17.1",
    "express-openapi-validator": "^2.15.0",
    "express-status-monitor": "^1.2.7",
    "http-status-codes": "^1.4.0",
    "pino": "^5.13.6"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.29",
    "@types/chai": "^4.2.5",
    "@types/express": "^4.17.2",
    "@types/mocha": "^5.2.7",
    "@types/pino": "^5.8.13",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "chai": "^4.2.0",
    "eslint": "^6.6.0",
    "mocha": "^6.2.2",
    "nodemon": "^1.19.4",
    "pino-pretty": "^3.3.0",
